{
    "manifest_version": 3,
    "name": "煎蛋无聊图",
    "version": "1.0.0",
    "description": "煎蛋无聊图扩展程序",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
    },
    "host_permissions": [
        "https://jandan.net/*"
    ],
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }  	
}